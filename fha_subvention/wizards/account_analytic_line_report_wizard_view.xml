<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="account_analytic_line_report_wizard_form" model="ir.ui.view">
        <field name="name">account.analytic.line.report.wizard.form</field>
        <field name="model">account.analytic.line.report.wizard</field>
        <field name="arch" type="xml">
            <form string="Timesheets Report Options">
                <group name="filters">
                    <group name="date_range">
                        <field name="date_from" />
                        <field name="date_to" />
                        <field name="analytic_group_id" />
                    </group>
                </group>
                <group name="other_filters">
                </group>
                <footer>
                    <button
                        name="button_export_pdf"
                        string="Export PDF"
                        type="object"
                        default_focus="1"
                        class="oe_highlight"
                    />
                    <button string="Cancel" special="cancel" />
                </footer>
            </form>
        </field>
    </record>
</odoo>
